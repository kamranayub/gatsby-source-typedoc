{
  "name": "gatsby-source-typedoc",
  "version": "1.2.1",
  "description": "A Gatsby source plugin that uses TypeDoc to compile to JSON exposed as GraphQL",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamranayub/gatsby-source-typedoc.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "source",
    "typedoc",
    "typescript"
  ],
  "author": "Kamran Ayub <me@kamranicus.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/kamranayub/gatsby-source-typedoc/issues"
  },
  "homepage": "https://github.com/kamranayub/gatsby-source-typedoc#readme",
  "peerDependencies": {
    "typedoc": ">= 0.17"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "jest": "^27.4.5",
    "prettier": "1.17.0",
    "standard-version": "^8.0.1",
    "typedoc": "^0.22.10",
    "typedoc17": "npm:typedoc@0.17.8",
    "typedoc21": "npm:typedoc@0.21.9",
    "typescript": "4.4.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
